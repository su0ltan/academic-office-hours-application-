        <activity android:name=".teacher.actReceivedRequest" />
        <activity android:name=".student.sendRequestAct" />
        <activity android:name=".main.activity_appointment" />
        <activity android:name=".student.Courses" />
        <activity
            android:name=".teacher.MainPageTeacher"
            android:label="@string/title_activity_main_page_teacher"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sultan.lasttest.MainActivity" />
        </activity>
        <activity
            android:name=".student.MainPage"
            android:label="@string/title_activity_main_page"
